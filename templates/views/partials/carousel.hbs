<h1>Carousel</h1>
<div id="carousel" class="carousel slide" data-ride="carousel">
  <!-- Wrapper for carousel slides -->
      <div id="articleCarouselInner"
          class="carousel-inner" role="listbox">
          {{#each carousel}}
          <!-- active class needs to be set just for first value -->
          {{#if @first}}
          <div class="item active">
          {{else}}
          <div class="item">
          {{/if}}
            <img class="carousel-image" src="{{{cloudinaryUrl image width=750 height=450 crop='fit' }}}">
          </div>
        {{/each}}
      </div>
  </div>
  <a class="carousel-control-prev" href="#carousel" role="button" data-slide="prev">
    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="carousel-control-next" href="#carousel" role="button" data-slide="next">
    <span class="carousel-control-next-icon" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>
